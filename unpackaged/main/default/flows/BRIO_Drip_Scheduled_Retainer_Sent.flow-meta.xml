        <name>tt_SOQL_outboundCommQuery</name>
        <isViewedAsPlainText>true</isViewedAsPlainText>
        <text>select id, name, Dwell_Time_Days_After_Date__c, Dwell_Time_Field__c, Case_Type__c,Record_Type_Name__c,RecordTypeId, BCC__c,CC__c, Email_From__c, Email_Template_ID__c, Intake_Status__c, Intake_Sub_Status__c, Last_Call_Result__c, Open_Email_Template__c, Recipient__c, SMS_Message_to_Client__c, LastModifiedById, LastModifiedDate, CreatedById, CreatedDate, Active__c, Active_For_Testing__c 
from Outbound_Communications__c
where record_type_name__c = &#39;Dwell Time&#39;
and Dwell_Time_Field__c =  &#39;{!const_dwellTimeField}&#39;
and Dwell_Time_Days_After_Date__c = {!var_dwellTmeDaysAfterDate}
and ( Active__c = true {!formula_activeForTesting}) 
and (Case_Type__c Includes (&#39;{!$Record.litify_pm__Case_Type__r.Name}&#39;) or Case_Type__c = null) 
and (Intake_Status__c Includes(&#39;{!$Record.litify_pm__Status__c}&#39;) or Intake_Status__c = null) 
and (Intake_Sub_Status__c Includes (&#39;{!$Record.Sub_Status__c}&#39;) or Intake_Sub_Status__c = null)</text>
