IF(CONTAINS({!$Record.five9DNIS__c}, &#39;0882&#39;), &#39;a03UJ000003vZnNYAU&#39;,
IF(CONTAINS({!$Record.five9DNIS__c}, &#39;1927&#39;), &#39;a03UJ00000bHU2nYAG&#39;,
