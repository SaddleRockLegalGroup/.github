    <decisions>
        <description>This decision will route the Asbestos intake to be assigned to Simmons Hanly Conroy which is currently out only Asbestos Partner.</description>
        <name>Is_Asbestos_Case_Type</name>
        <label>Is Asbestos Case Type?</label>
        <locationX>182</locationX>
        <locationY>539</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Case_Type_is_Asbestos</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>getUpdatedIntake.Case_Type_Name_del__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Asbestos</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Intake_Vendor_Record</targetReference>
            </connector>
            <label>Case Type is Asbestos</label>
        </rules>
