IF(CONTAINS({!$Record.five9DNIS__c}, &#39;3474&#39;), &#39;a03Dn0000054CCKIA2&#39;,
IF(CONTAINS({!$Record.five9DNIS__c}, &#39;2462&#39;), &#39;a03UJ00000Nx22KYAR&#39;,
IF(CONTAINS({!$Record.five9DNIS__c}, &#39;4479&#39;), &#39;a03UJ000003vZnMYAU&#39;,
