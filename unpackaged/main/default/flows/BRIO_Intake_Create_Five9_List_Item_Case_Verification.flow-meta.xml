<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>60.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <decisions>
        <name>Existing_List_Item_Found</name>
        <label>Existing List Item Found?</label>
        <locationX>176</locationX>
        <locationY>539</locationY>
        <defaultConnector>
            <targetReference>Remove_Five9_List_Item</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>yes_existing_list_item_found</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Existing_Five9_List_Item.Id</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <label>yes, existing list item found</label>
        </rules>
    </decisions>
    <description>When an Intake is created or updated and Status = &#39;Case Verification&#39;, this flow creates a new Five9 List Item to sync the Intake to the Case Verification call queue | Added &#39;External&#39; Record type exclusion</description>
    <environments>Default</environments>
    <interviewLabel>BRIO {!$Flow.CurrentDateTime}</interviewLabel>
    <label>BRIO | 3P | Create Five9 List Item - Case Verification</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordLookups>
        <name>Get_Existing_Five9_List_Item</name>
        <label>Get Existing Five9 List Item</label>
        <locationX>176</locationX>
        <locationY>431</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Existing_List_Item_Found</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Intake__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.Id</elementReference>
            </value>
        </filters>
        <filters>
            <field>Five9LSP__Five9_List__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>Get_Five9_List.Id</elementReference>
            </value>
        </filters>
        <getFirstRecordOnly>true</getFirstRecordOnly>
        <object>Five9LSP__Five9_List_Item__c</object>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordLookups>
    <recordLookups>
        <name>Get_Five9_List</name>
        <label>Get Five9 List</label>
        <locationX>176</locationX>
        <locationY>323</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Get_Existing_Five9_List_Item</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Name</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>CV_Litify</stringValue>
            </value>
        </filters>
        <getFirstRecordOnly>true</getFirstRecordOnly>
        <object>Five9LSP__Five9_List__c</object>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordLookups>
    <start>
        <locationX>50</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Get_Five9_List</targetReference>
        </connector>
        <filterFormula>NOT(ISBLANK({!$Record.Intake_Vendor__c})) 
&amp;&amp; {!$Record.Intake_Vendor__r.Name} &lt;&gt; &#39;Saddle Rock&#39; 
&amp;&amp; {!$Record.Intake_Vendor__r.Name} &lt;&gt; &#39;SaddleRock&#39; 
&amp;&amp; {!$Record.RecordType.DeveloperName} &lt;&gt; &#39;External&#39; 
&amp;&amp; ISPICKVAL({!$Record.litify_pm__Status__c}, &#39;Case Verification&#39;) 
&amp;&amp; (ISNEW() || ISCHANGED({!$Record.litify_pm__Status__c}))</filterFormula>
        <object>litify_pm__Intake__c</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Draft</status>
    <subflows>
        <name>Remove_Five9_List_Item</name>
        <label>Remove Five9 List Item</label>
        <locationX>308</locationX>
        <locationY>647</locationY>
        <flowName>Brio_Five9List_Item_Insert_and_Removal</flowName>
        <inputAssignments>
            <name>varFive9ListItemId</name>
            <value>
                <elementReference>Get_Five9_List.Id</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>varFlowName</name>
            <value>
                <stringValue>BRIO | 3P | Create Five9 List Item</stringValue>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>varIntake</name>
            <value>
                <elementReference>$Record.FX_Id__c</elementReference>
            </value>
        </inputAssignments>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </subflows>
    <variables>
        <name>Flow_Name</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>false</isInput>
        <isOutput>false</isOutput>
        <value>
            <stringValue>BRIO | 3P | Create Five9 List Item - Case Verification</stringValue>
        </value>
    </variables>
</Flow>
