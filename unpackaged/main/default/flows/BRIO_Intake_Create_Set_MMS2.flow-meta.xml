    <textTemplates>
        <name>tt_mmsSoqlQuery</name>
        <isViewedAsPlainText>true</isViewedAsPlainText>
        <text>SELECT Id
FROM litify_mktgROI__Monthly_Marketing_Spend__c 
WHERE litify_mktgROI__Source__c = &#39;{!$Record.litify_pm__Source__c}&#39;
AND litify_mktgROI__Monthly_Spend_Start_Date__c &lt;= {!fx_intakeCreatedDate}
AND End_Date__c &gt;= {!fx_intakeCreatedDate}
AND Case_Type__c INCLUDES (&#39;{!$Record.litify_pm__Case_Type__r.Name}&#39;)
{!fx_finalSoqlWhereClause}</text>
