    <description>3/28/2025 - CS - Added logging to debug SOQL error.
12/11/2024 CS - Updates to flow to account for edge case where Intake matches one or more than one Campaign partner, but those Campaign Partners records distribution percentage is lower than the current assignments (i.e. distribution percentage on campaign record is lowered to something like 25%, but the still have a higher percentage than that of the total assignments)</description>
    <environments>Default</environments>
    <formulas>
        <name>fx_CampaignPartnerSOQLQuery</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK({!Loop_Campaign_Partners.Criteria__c}), &quot;SELECT Id FROM litify_pm__Intake__c WHERE ID = &#39;&quot; + {!recordId}  + &quot;&#39;&quot; , IF(FIND( &quot;WHERE&quot;,{!Loop_Campaign_Partners.Criteria__c}) &gt; 0, SUBSTITUTE({!Loop_Campaign_Partners.Criteria__c}, &quot;WHERE&quot;, &quot;WHERE (&quot;) + &quot;) and id =&#39;&quot;+ {!recordId} + &quot;&#39;&quot;, {!Loop_Campaign_Partners.Criteria__c} + &quot; WHERE id = &#39;&quot; +  {!recordId} + &quot;&#39;&quot;))</expression>
    </formulas>
    <formulas>
        <name>fx_CPNewIntakesAssignedCount</name>
        <dataType>Number</dataType>
        <expression>IF(ISNULL( {!loop_sorted_campaign_partners.Intakes_Assigned__c}), 1, {!loop_sorted_campaign_partners.Intakes_Assigned__c} +1)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>fx_difference</name>
        <dataType>Number</dataType>
        <expression>{!fxCeilingTest} - {!fxExactIntakeNumber}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>fx_NewIntakeAssignedCount</name>
        <dataType>Number</dataType>
        <expression>IF( ISNULL({!Loop_Through_Campaign_Partner_Matches.Intakes_Assigned__c}), 1, {!Loop_Through_Campaign_Partner_Matches.Intakes_Assigned__c} +1)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>fx_NewTotalIntakeCount</name>
        <dataType>Number</dataType>
        <expression>IF( ISNULL( {!Get_Campaign.Total_Intakes_Assigned__c}), 1, {!Get_Campaign.Total_Intakes_Assigned__c} +1)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>fxActualPercentage</name>
        <dataType>Number</dataType>
        <expression>IF(ISNULL({!Loop_Through_Campaign_Partner_Matches.Intakes_Assigned__c}), 0 * 100/ {!fxNewIntakeCount}, {!Loop_Through_Campaign_Partner_Matches.Intakes_Assigned__c} * 100/ {!fxNewIntakeCount})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>fxCeilingTest</name>
        <dataType>Number</dataType>
        <expression>CEILING({!fxExactIntakeNumber})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>fxdifference2</name>
        <dataType>Number</dataType>
        <expression>IF(ISNULL({!Loop_Through_Campaign_Partner_Matches.Intakes_Assigned__c}), ABS(0 - {!fxExactIntakeNumber}), ABS({!Loop_Through_Campaign_Partner_Matches.Intakes_Assigned__c} - {!fxExactIntakeNumber}))</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>fxExactIntakeNumber</name>
        <dataType>Number</dataType>
        <expression>{!fxNewIntakeCount} * {!fxPercent}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>fxNewIntakeCount</name>
        <dataType>Number</dataType>
        <expression>IF(ISBLANK({!Get_Intake.Campaign__r.Total_Intakes_Assigned__c}), 1, {!Get_Intake.Campaign__r.Total_Intakes_Assigned__c} + 1)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>fxNewIntakesAssignedCount</name>
        <dataType>Number</dataType>
        <expression>{!loop_sorted_campaign_partners.Intakes_Assigned__c} + 1</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>fxPercent</name>
        <dataType>Number</dataType>
        <expression>{!Loop_Through_Campaign_Partner_Matches.Distribution_Percent__c}/100</expression>
        <scale>2</scale>
    </formulas>
