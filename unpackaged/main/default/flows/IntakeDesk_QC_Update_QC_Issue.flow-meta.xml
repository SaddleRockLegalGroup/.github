<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>63.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <decisions>
        <name>Changed_to_Passed</name>
        <label>Changed to Passed</label>
        <locationX>314</locationX>
        <locationY>287</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Clear_QC_Issue</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Intake_Desk_Review__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Passed</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_QC_Issue_to_None</targetReference>
            </connector>
            <label>Clear QC Issue</label>
        </rules>
        <rules>
            <name>Reset_QC_Issue_to_EOF</name>
            <conditionLogic>(1 OR 2 OR 3 OR 4) AND 5</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Intake_Desk_Review__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>EOF - D&amp;C</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Intake_Desk_Review__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>EOF - ID/D&amp;C</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Intake_Desk_Review__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>EOF - DQ/Dupe</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Intake_Desk_Review__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>EOF - ID</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.QC_Type__c</leftValueReference>
                <operator>IsBlank</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_QC_Issue_to_EOF</targetReference>
            </connector>
            <label>Reset QC Issue to any EOF</label>
        </rules>
    </decisions>
    <description>5/15 LP Added a decision to allow for resetting of QC issue when changed back to an EOF option after being cleared from being change to passed.
Update QC Issue to -None- when Intake Desk Review is set to Approved</description>
    <environments>Default</environments>
    <interviewLabel>IntakeDesk QC | Update QC Issue {!$Flow.CurrentDateTime}</interviewLabel>
    <label>IntakeDesk QC | Update QC Issue</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <description>Update QC Issue to -EOF- when Intake Desk Review is set to any EOF option</description>
        <name>Update_QC_Issue_to_EOF</name>
        <label>Update QC Issue to EOF</label>
        <locationX>314</locationX>
        <locationY>395</locationY>
        <inputAssignments>
            <field>QC_Type__c</field>
            <value>
                <stringValue>EOF</stringValue>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <recordUpdates>
        <description>Update QC Issue to -None- when Intake Desk Review is set to Approved</description>
        <name>Update_QC_Issue_to_None</name>
        <label>Update QC Issue to None</label>
        <locationX>50</locationX>
        <locationY>395</locationY>
        <inputAssignments>
            <field>QC_Type__c</field>
            <value>
                <stringValue></stringValue>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <start>
        <locationX>188</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Changed_to_Passed</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Intake_Desk_Review__c</field>
            <operator>IsChanged</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <filters>
            <field>Intake_Vendor__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>a2pUJ000002ngBRYAY</stringValue>
            </value>
        </filters>
        <object>litify_pm__Intake__c</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordBeforeSave</triggerType>
    </start>
    <status>Active</status>
</Flow>
