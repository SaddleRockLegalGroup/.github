<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>61.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <decisions>
        <name>Which_Reason</name>
        <label>Which Reason</label>
        <locationX>446</locationX>
        <locationY>323</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>New_Jersey</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.State_of_Residency__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>New Jersey</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Intake__r.Case_Type_Name_del__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Zantac</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Intake</targetReference>
            </connector>
            <label>New Jersey</label>
        </rules>
        <rules>
            <name>Florida</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.State_of_Residency__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Florida</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Intake__r.Case_Type_Name_del__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Zantac</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Copy_1_of_Update_Intake</targetReference>
            </connector>
            <label>Florida</label>
        </rules>
        <rules>
            <name>Paraquat_Married</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Intake__r.Case_Type_Name_del__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Paraquat</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Copy_3_of_Update_Intake</targetReference>
            </connector>
            <label>Paraquat Married</label>
        </rules>
    </decisions>
    <description>Used to update the Non-standard checkbox on intake when some choice in the campaign case is chosen that requires docs not normal. Added Florida Option
added Paraquat 11/15/24</description>
    <environments>Default</environments>
    <interviewLabel>SRLG | Campaign Case | Non-Standard doc scenario {!$Flow.CurrentDateTime}</interviewLabel>
    <label>SRLG | Campaign Case | Non-Standard doc scenario</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <name>Copy_1_of_Update_Intake</name>
        <label>Copy 1 of Update Intake</label>
        <locationX>314</locationX>
        <locationY>431</locationY>
        <inputAssignments>
            <field>Current_Florida_Resident__c</field>
            <value>
                <stringValue>Yes</stringValue>
            </value>
        </inputAssignments>
        <inputReference>$Record.Intakes__r</inputReference>
    </recordUpdates>
    <recordUpdates>
        <name>Copy_3_of_Update_Intake</name>
        <label>Copy 3 of Update Intake</label>
        <locationX>578</locationX>
        <locationY>431</locationY>
        <inputAssignments>
            <field>Non_Standard__c</field>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </inputAssignments>
        <inputReference>$Record.Intakes__r</inputReference>
    </recordUpdates>
    <recordUpdates>
        <name>Update_Intake</name>
        <label>Update Intake</label>
        <locationX>50</locationX>
        <locationY>431</locationY>
        <inputAssignments>
            <field>Non_Standard__c</field>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </inputAssignments>
        <inputReference>$Record.Intakes__r</inputReference>
    </recordUpdates>
    <start>
        <locationX>320</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Which_Reason</targetReference>
        </connector>
        <filterFormula>({!$Record.Case_Type__r.Name} = &quot;Zantac&quot; 
&amp;&amp;
(
({!$Record.Intake__r.Non_Standard__c} = FALSE
&amp;&amp;
ISPICKVAL({!$Record.State_of_Residency__c} , &quot;New Jersey&quot;)
)
||
(
ISPICKVAL({!$Record.Intake__r.Current_Florida_Resident__c}, &quot;No&quot;)
&amp;&amp;
ISPICKVAL({!$Record.State_of_Residency__c} , &quot;Florida&quot;)
)
))
||
(AND( {!$Record.Intake__r.litify_pm__Case_Type__r.Name} = &quot;Paraquat&quot;,
{!$Record.Intake__r.Non_Standard__c} = FALSE,
ISPICKVAL({!$Record.Intake__r.litify_pm__Client__r.Marital_Status__c}, &quot;Married&quot;)
)
)</filterFormula>
        <object>Campaign_Case__c</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Active</status>
</Flow>
