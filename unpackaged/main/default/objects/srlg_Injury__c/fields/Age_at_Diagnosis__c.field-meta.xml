<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Age_at_Diagnosis__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
  ISBLANK(Diagnosis_Date__c) || ISBLANK(Intake__r.Injured_Party_Birthdate__c),
  null,
  FLOOR(
    (Diagnosis_Date__c - Intake__r.Injured_Party_Birthdate__c) / 365.25
  )
)</formula>
    <label>Age at Diagnosis</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
